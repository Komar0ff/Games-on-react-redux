{
	"name": "games-on-react",
	"version": "0.3.0",
	"description": "games on react-effector",
	"repository": {
		"type": "git",
		"url": "https://github.com/Komar0ff/games-on-react"
	},
	"keywords": [
		"games",
		"game",
		"react",
		"svelte",
		"effector"
	],
	"author": {
		"name": "Komar0ff",
		"email": "binary.reptile@gmail.com",
		"url": "https://twitter.com/ilya_komar0ff"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Komar0ff/games-on-effector/issues"
	},
	"dependencies": {
		"effector": "20.4.0",
		"effector-react": "20.4.0",
		"linaria": "1.4.0-alpha.1",
		"linaria-jest": "^0.0.4",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-router": "^5.1.2",
		"router5": "7.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@babel/runtime": "^7.6.3",
		"@testing-library/react": "^9.3.0",
		"babel-jest": "^24.9.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"compression-webpack-plugin": "^3.0.0",
		"css-loader": "^3.2.0",
		"fast-async": "^6.3.8",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^3.0.8",
		"jest": "^24.9.0",
		"jsdom": "^15.1.1",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"style-loader": "^1.0.0",
		"terser-webpack-plugin": "^2.1.3",
		"url-loader": "^2.2.0",
		"webpack": "4.41.1",
		"webpack-bundle-analyzer": "^3.5.2",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "3.8.2",
		"webpack-merge": "^4.2.2",
		"whatwg-fetch": "^3.0.0"
	},
	"scripts": {
		"dev": "webpack-dev-server --hot --progress --color --config config/webpack.dev.config.js",
		"build": "yarn format && webpack --mode production --progress --color --config config/webpack.prod.config.js",
		"format": "prettier --write \"src/**/*.js\"",
		"test": "jest --watch",
		"deploy": "yarn build && now ./build"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged",
			"pre-push": "yarn jest"
		}
	},
	"jest": {
		"testEnvironment": "jsdom",
		"setupFilesAfterEnv": [
			"<rootDir>/config/jest/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/__mocks__/fileMock.js",
			"\\.(css|scss)$": "<rootDir>/config/jest/__mocks__/styleMock.js"
		},
		"moduleDirectories": [
			"src",
			"node_modules"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"jsx"
		],
		"transform": {
			"^.+\\.(js|jsx)$": "babel-jest"
		}
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	}
}
